/**
 * Существует несколько датчиков с уникальными серийными номерами. 
 * Каждый датчик снимает показания с счетчиков холодной и горячей воды и шлет показания пачкой.
 *
 * Формат JSON, который шлет датчик на сервер:
 * sn					серийный номер датчика
 * data					массив показаний (количество элементов в массиве не ограниченно)
 * 		readout_dt		время снятия показаний датчика с счетчиков
 *		id				уникальный идентификатор показания
 *		counters		показания с счетчиков горячей и холодной воды
 *			val			абсолютное показание счетчика
 *			n			тип счетчика:	1 - счетчик холодной воды
 *										2 - счетчик горячей воды
 */
{
	"sn": "13817406", 
	"data": [
		{
			"readout_dt": "1465193586", 
			"id": "881", 
			"counters": [
				{
					"val": "2877", 
					"n": "1"
				}, {
					"val": "2237", 
					"n": "2"
				}
			]
		}, {
			"readout_dt": "1465197765", 
			"id": "882", 
			"counters": [
				{
					"state": "0", 
					"val": "2878", 
					"n": "1"
				}, {
					"state": "1", 
					"val": "2238", 
					"n": "2"
				}
			]
		}
	]
}